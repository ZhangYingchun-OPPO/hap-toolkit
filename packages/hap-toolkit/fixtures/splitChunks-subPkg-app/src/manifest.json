{
  "package": "com.application.demo",
  "name": "subpackages-splitChunks-demo",
  "versionName": "1.0.0",
  "versionCode": "1",
  "minPlatformVersion": 1070,
  "icon": "/logo.png",
  "features": [
    { "name": "system.prompt" },
    { "name": "system.router" },
    { "name": "system.shortcut" }
  ],
  "permissions": [
    { "origin": "*" }
  ],
  "config": {
    "logLevel": "debug"
  },
  "router": {
    "entry": "Sub1/page1",
    "pages": {
      "Sub1/page1": {
        "component": "index"
      },
      "Sub1/page2": {
        "component": "index"
      },
      "Sub2/page1": {
        "component": "index"
      },
      "Sub2/page2": {
        "component": "index"
      },
      "Sub3": {
        "component": "index"
      }
    }
  },
  "subpackages": [
    {
      "name": "sub2",
      "resource": "Sub2"
    },
    {
      "name": "sub3",
      "resource": "Sub3",
      "standalone": true
    }
  ],
  "display": {
    "titleBarBackgroundColor": "#f2f2f2",
    "titleBarTextColor": "#414141",
    "menu": true,
    "pages": {
      "Demo": {
        "titleBarText": "示例页",
        "menu": false
      },
      "DemoDetail": {
        "titleBarText": "详情页"
      },
      "About": {
        "menu": false
      }
    }
  }
}